{
  "manifest_version": 2,

  "name": "downloading_is_sick",
  "description": "lets_be_lazy",
  "version": "1.0",
  "background":{
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons":{
	"16": "icon16.png",
  	"48": "icon48.png",
  	"128": "icon128.png"
  },
  "permissions": [
    "storage","activeTab","tabs"
  ]
}
